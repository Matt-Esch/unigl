{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Windows Debug x86 Dynamic",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/MDd",
				"/EHsc",
				"/D_UNICODE",
				"/DUNICODE",
				"/DWIN32",
				"/D_WINDOWS",
				"/DDYNAMIC_ANNOTATIONS_ENABLED=1",
				"/D_HAS_ITERATOR_DEBUGGING=0",
				"/I${workspaceFolder}\\include\\",

				"/Fe:",
				"${fileDirname}\\${fileBasenameNoExtension}.exe",

				"${file}",

				"advapi32.lib",
				"comdlg32.lib",
				"dbghelp.lib",
				"dnsapi.lib",
				"gdi32.lib",
				"msimg32.lib",
				"odbc32.lib",
				"odbccp32.lib",
				"oleaut32.lib",
				"shell32.lib",
				"shlwapi.lib",
				"user32.lib",
				"usp10.lib",
				"uuid.lib",
				"version.lib",
				"wininet.lib",
				"winmm.lib",
				"winspool.lib",
				"ws2_32.lib",
				"delayimp.lib",
				"kernel32.lib",
				"ole32.lib",
				"d3d9.lib",
				"dxguid.lib",
				"setupapi.lib",
				"dxgi.lib",
				"bin/windows/x86/release/libEGL.dll.lib",
				"bin/windows/x86/release/libGLESv2.dll.lib",

				"/link",
				"/OPT:REF",
				"/OPT:ICF",
				"/DEBUG",
				"/INCREMENTAL:NO",
				"/FIXED$:NO",
				"/PROFILE",
				"/WX",
				"/OPT:ICF",
				"/MACHINE:X86",
				"/SAFESEH",
				"/largeaddressaware",
				"/FIXED:NO",
				"/ignore:4199",
				"/ignore:4221",
				"/NXCOMPAT",
				"/DELAYLOAD:d3d9.dll",
				"/LIBPATH:C:/Program Files (x86)/Windows Kits/10/lib/10.0.18362.0/um/x86",
				"/LIBPATH:C:/Program Files (x86)/Microsoft Visual Studio/2019/Community/VC/Tools/MSVC/14.25.28610/lib/x86",
				"/LIBPATH:C:/Program Files (x86)/Microsoft Visual Studio/2019/Community/VC/Tools/MSVC/14.25.28610/ATLMFC/lib/x86"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
